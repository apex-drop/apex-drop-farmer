[
  {
    "id": 1,
    "action": {
      "type": "redirect",
      "redirect": {
        "regexSubstitution": "&tgWebAppPlatform=android"
      }
    },
    "condition": {
      "regexFilter": "&tgWebAppPlatform=web"
    }
  },
  {
    "id": 2,
    "action": {
      "type": "modifyHeaders",
      "requestHeaders": [
        {
          "header": "user-agent",
          "operation": "set",
          "value": "Mozilla/5.0 (Linux; Android 14; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/128.0.6613.127 Mobile Safari/537.36"
        }
      ],
      "responseHeaders": [
        {
          "header": "content-security-policy",
          "operation": "remove"
        },
        {
          "header": "x-frame-options",
          "operation": "remove"
        },
        {
          "header": "access-control-expose-headers",
          "operation": "set",
          "value": "*"
        }
      ]
    },
    "condition": {
      "urlFilter": "|*",
      "resourceTypes": ["sub_frame", "xmlhttprequest"]
    }
  }
]
